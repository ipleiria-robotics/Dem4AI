n = 30
X = np.array( [[0.93,0.22], [0.74,0.61], [0.83,0.85], [0.91,0.2], [0.63,0.9], [0.71,0.84], [0.73, 0.38], [0.53,0.68], [0.62,0.71], [0.81,0.29]] )
